{
  "name": "dragonflies-graphql-api",
  "version": "1.0.1",
  "description": "A GraphQL API",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "graphql": "^16.8.1"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@as-integrations/next": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "lockfile-lint": "^4.12.1",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nock": "^13.3.8",
    "node-fetch": "^2.7.0",
    "prettier": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/primavera133/dragonsGraphQLAPI.git"
  },
  "scripts": {
    "dev": "node api/dev-server.js",
    "build": "echo 'Build complete'",
    "test": "npm run lockfile-lint && npm run jest",
    "jest": "jest --forceExit --detectOpenHandles ",
    "update-snaps": "jest --updateSnapshot",
    "lockfile-lint": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "generate-gql-types": "graphql-codegen"
  },
  "keywords": [
    "Dragonflies",
    "graphql"
  ],
  "author": "Jonas Myren√•s",
  "license": "ISC"
}
